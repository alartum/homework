#include <stn.dlib>

fib(eax)
{
    ecx = 1;
    edx = 0;
    
    ebp = 0;
    
    while (ebx < eax)
    {
        ebx = ecx;
        ecx = ecx + ebx;
        edx = ebx;
        ebp = ebp + 1;
    }
    
    return (ecx);
}

